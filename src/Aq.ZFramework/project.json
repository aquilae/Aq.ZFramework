{
  "version": "0.0.1-*",

  "title": "Aq.NetMQ",
  "authors": [
    "Ilya \"aq\" Kozlov"
  ],
  "description": "Very basic web-like framework and server using ZeroMQ",

  "packOptions": {
    "projectUrl": "https://github.com/aquilae/Aq.ZFramework",
    "repository": {
      "type": "git",
      "url": "https://github.com/aquilae/Aq.ZFramework"
    },
    "summary": "Very basic web-like framework and server using ZeroMQ",
    "licenseUrl": "https://github.com/aquilae/Aq.ZFramework/blob/master/LICENSE",
    "owners": [
      "Ilya \"aq\" Kozlov"
    ],
    "tags": [
      "NetMQ",
      "ZeroMQ"
    ]
  },

  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [
          "DEBUG",
          "TRACE"
        ],
        "optimize": false,
        "warningsAsErrors": true
      }
    },
    "Release": {
      "buildOptions": {
        "define": [
          "RELEASE"
        ],
        "optimize": true,
        "warningsAsErrors": true
      }
    }
  },

  "dependencies": {
    "Microsoft.Extensions.DependencyInjection": "*",
    "Microsoft.Extensions.DependencyInjection.Abstractions": "*",
    "Aq.NetMQ": "0.0.1"
  },

  "frameworks": {
    "net45": {
      "dependencies": {
        "System.Runtime": "4.*"
      }
    },
    "netstandard1.3": {
      "imports": "dnxcore50",
      "dependencies": {
        "NETStandard.Library": "1.6"
      }
    }
  }
}
